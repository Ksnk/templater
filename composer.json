{

  "version": "1.1.0",
  "name": "ksnk/templater",
  "description": "Jinja2 php templater engine",
  "keywords": ["ksnk", "jinja", "templates"],
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Serge Koriakin",
      "email": "sergekoriakin@gmail.com",
      "homepage": "https://github.com/ksnk"
    }
  ],
  "require": {
    "php": ">=5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8.35 || ^5.7 || ^6.4 || ^7.0"
  },
  "autoload": {
    "psr-4": {
      "Ksnk\\templater\\": "src/",
      "Ksnk\\scaner\\": "scaner/",
      "Ksnk\\templates\\": "templates/"
    }
  },

  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true

}
